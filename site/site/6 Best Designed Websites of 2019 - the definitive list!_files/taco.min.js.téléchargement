jQuery(document).ready(function(){jQuery(document).on("click",'a[href*="#"]',function(){if(location.pathname.replace(/^\//,"")==this.pathname.replace(/^\//,"")&&location.hostname==this.hostname){var t="";if("prevent"===jQuery(this).attr("data-scroll"))return!1;if("#go-top"===this.hash){var e=jQuery("#taco-section-menu");t=e.length?e:jQuery("body")}else t=jQuery(this.hash);var a=parseInt(jQuery(this).attr("data-taco-scroll-margin"));if(isNaN(a)&&(a=0),(t=t.length?t:jQuery("[name="+this.hash.slice(1)+"]")).length)return jQuery("html, body").animate({scrollTop:t.offset().top-a},400),!1}}),jQuery('[data-action="content-toggle"]').click(function(t){t.preventDefault(),jQuery(this).toggleClass("off"),jQuery(this).toggleClass("on");var e=jQuery(this).attr("href");jQuery(e).toggleClass("off"),jQuery(e).toggleClass("on")})});